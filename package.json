    "@remix-run/node": "^2.12.1",
    "@remix-run/react": "^2.12.1",
    "@remix-run/serve": "^2.12.1",
    "aws-cdk-lib": "2.159.1",
    "aws-lambda": "^1.0.7",
    "constructs": "^10.0.0",
    "esbuild": "^0.24.0",
    "isbot": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-aws": "^1.1.0"
  },
  "devDependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "^3.658.1",
    "@aws-sdk/client-dynamodb": "^3.658.1",
